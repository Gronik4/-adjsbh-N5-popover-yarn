{
    "env": {
      "test": {
        "presets": [[
          "@babel/preset-env",{
            "targets": {"node": "current"}
            }
         ]]
      }
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": ["error", "windows"], //Перенос строк. Удалить перд push!!!!!!!
        "no-unused-vars": "off",
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
          ],
          "import/no-extraneous-dependencies": [
            "error",
            {
              "devDependencies": true
            }
          ]
    }
}
